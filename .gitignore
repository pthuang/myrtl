#######################################################
#                                                      
#       git management rule Settings                   
#       ! means need add to git                        
#       otherwise means need to ignore                 
#                                                      
# Can test by "git status -u" check if rules valid   
#                                                      
#######################################################
# ignore all files in root directory
/*
# except ".gitignore" file 
!.gitignore
# except ".readme.md" file 
!readme.md
# except map directory
!/map/
# ignore myrtl_test directory in map directory
/map/myrtl_test/*
/map/*.bat
/map/*.jou
/map/*.log
/map/*.str
!/map/vitis/
/map/vitis/*
!/map/vitis/*.xsa
!/map/vitis/*.bit
!/map/vitis/myrtl_test/
/map/vitis/myrtl_test/*
!/map/vitis/myrtl_test/src/
/map/vitis/myrtl_test/src/*
!/map/vitis/myrtl_test/src/*.c
!/map/vitis/myrtl_test/src/*.h
!/map/vitis/myrtl_test/src/*.ld
# script directory
!/script/
/script/*
!/script/tb_arbitor_do/
!/script/tb_m_spi_do/
!/script/tb_spi_do/
!/script/tb_uart_do/
!/script/*.bat
!/script/*.do
# sim directory
!/sim/
# src directory
!/src/
/src/bd/*
# bd directory
!/src/bd/*.tcl
# coe directory
!/src/coe/*
# constrs directory
!/src/constrs/*
# elf directory
!/src/elf/*
# src code directory
!/src/imports/*
# ip directory
# ip_repo directory
!/src/ip_repo/*
